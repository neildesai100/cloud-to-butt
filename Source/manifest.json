{
	"manifest_version": 2,
	"name": "Corona To 🅱️orona",
	"version": "1.0",
	"description": "Replaces the text 'coronavirus' with '🅱️oronavirus'.",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
	"page_action": {
		"default_icon": {
		  "16": "images/logo16x16.png",
		  "32": "images/logo32x32.png",
		  "128": "images/logo128x128.png"
		}
	  }
}

